                       <input id="search1" type="text" placeholder="Search A Product">

                       <div id="exam1" class="ex1">
                           <table id="exc">
                             <!-- <tr><td class="searchPro"><a href="#">Search Product 1</a></td></tr> -->
                           </table>
                       </div>

                   </div>
                   <div class="modal-footer">
                       <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                   </div>
               </div>
           </div>
 </div>
