                     <td><div id="rdiv1"></div><div id="rdiv2" onmouseover="ch(this)" onmouseout="ch1(this)" onclick="gyb()"><b>Remove all</b></div></td>
                 </tr>
                 <tr>
                     <td id='td1'>
                         <div id="cdiv1" class="cdiv"><center><div class="cinside" onmouseover="dark1(this)"
                           onmouseout="light1(this)" data-toggle="modal" data-target="#myModal">Add Another Product</div></center></div>
                     </td>
                     <td id='td2'>
                         <div id="cdiv2" class="cdiv"><center><div class="cinside" onmouseover="dark1(this)" onmouseout="light1(this)" data-toggle="modal" data-target="#myModal">Add Another Product</div></center></div>
                     </td>
                     <td id='td3'>
                         <div id="cdiv3" class="cdiv"><center><div class="cinside" onmouseover="dark1(this)" onmouseout="light1(this)" data-toggle="modal" data-target="#myModal">Add Another Product</div></center></div>
                     </td>
                     <td id='td4'>
                         <div id="cdiv4" class="cdiv"><center><div class="cinside" onmouseover="dark1(this)" onmouseout="light1(this)" data-toggle="modal" data-target="#myModal">Add Anoter Product</div></center></div>
                     </td>
                     <td><div class="cdiv1"><button class="button button4" onmouseover="b1(this)" onmouseout="d1(this)">Compare Now</button></div></td>
                 </tr>
             </table>
         </div>
     </center>

    <div id="sidemid" class="container-fluid mn">
      <% include sidebar.ejs %>
        <div class="side1">
          <div class="sidebarright">
            Laptops
          </div>
          <div id="fill">
          <br>
          <% for(var i=0 ; i<l ; i++)
          {
            var ob = JSON.stringify(pp[i].specifications.full_specs);
            var ob1 = JSON.parse(ob);
            %>
          <div class="product">
            <div class="left_product">
              <center><a href="http://localhost:3000/pro/<%=pp[i].key%>"><img src="<%=pp[i].img%>" alt="no laptop" width="100" height="100"></a></center>
            </div>
            <div class="mid_product">
              <div class="mid_product_up">
                <a href="http://localhost:3000/pro/<%=pp[i].key%>" class="nm"><%=pp[i].name%></a>
              </div>
              <div class="mid_product_down">
                <div class="mid_product_down_up">
                  <div class="mid_product_down_up_left">
                    <b>Product Features:</b>
                  </div>
                  <div class="mid_product_down_up_right">
                    <a href="http://localhost:3000/pro/<%=pp[i].key%>">See Full Specifications</a>
                  </div>
                </div>
                <div style="height:2px"></div>
                <div class="mid_product_down_down">
                  <div class="mid_product_down_down_left">
                    <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=ob1.Processor.Processor%>
                    <br>
                    <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=ob1.Processor.Speed%> Clock Speed
                    <br>
                    <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=ob1.Memory.RAM%> RAM
                    <br>
                    <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=ob1.Memory['Hard Disk Capacity']%> Hard Disk
                  </div>
                  <div class="mid_product_down_down_right">
                    <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%
                    if(ob1.Graphics!=undefined)
                    {
                    %>
                    <%= ob1.Graphics.GPU %>
                    <%}else{%>
                      <%= ob1.Battery['Battery Backup'] %> Battery Backup
                  <%}%>
                    <br>
                    <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=ob1.Display.Size%>, <%=ob1.Display.Resolution%>
                    <br>
                    <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=ob1.General.OS%> OS
                    <br>
                    <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=ob1.General.Warranty%>
                  </div>
                </div>
              </div>
            </div>
            <div class="right_product">
              <div class="right_product_up">
                <center><b>Rs. <%=pp[i].price%></b></center>
              </div>
              <div class="right_product_down">
                <center>
                  <button type="button" class="<%=pp[i].key%>" id="comp" name="compare" onclick="openCompare(this)"><i class="fa fa-plus-square" aria-hidden="true">
                    </i>&nbsp;compare
                  </button>
                  <button type="button" class="<%=pp[i].key%>" id="comp1" name="compare" onclick="openCompare1(this)"><i class="fa fa-remove" aria-hidden="true">
                    </i>&nbsp;compare
                  </button>
                </center>
              </div>
            </div>
          </div>
          <div style="height:2px"></div>
          <%}%>
          <center>
          <%
          if (pages > 0) { %>
                <ul class="pagination text-center">
                    <% if (current == 1) { %>
                        <li class="disabled"><a>First</a></li>
                    <% } else { %>
                        <li><a href="/1">First</a></li>
                    <% } %>
                    <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                    <% if (i !== 1) { %>
                        <li class="disabled"><a>...</a></li>
                    <% } %>
                    <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                        <% if (i == current) { %>
                            <li class="active"><a><%= i %></a></li>
                        <% } else { %>
                            <li><a href="/<%= i %>"><%= i %></a></li>
                        <% } %>
                        <% if (i == Number(current) + 4 && i < pages) { %>
                            <li class="disabled"><a>...</a></li>
                        <% } %>
                    <% } %>
                    <% if (current == pages) { %>
                        <li class="disabled"><a>Last</a></li>
                    <% } else { %>
                        <li><a href="/<%= pages %>">Last</a></li>
                    <% } %>
                </ul>
            <% } %>
          </center>
          </div>            </div>
          </div>

