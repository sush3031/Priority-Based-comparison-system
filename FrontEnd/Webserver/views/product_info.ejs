<!DOCTYPE html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%= pp.name %></title>
    <title>Home Page</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link rel="stylesheet" href="../stylesheets/Header.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </head>
  <body>
    <% include header %>
    <div style="height:3px"></div>

    <div class="row product_info">
      <div class="row">
        <div class="col-sm-1 col-md-1 col-lg-1"></div>
        <div class="col-sm-10 col-md-10 col-lg-10 nm1">
          <%= pp.name %>
        </div>
        <div class="col-sm-1 col-md-1 col-lg-1"></div>
      </div>
      <br>
      <div class="row">
        <div class="col-sm-1 col-md-1 col-lg-1"></div>
        <div class="col-sm-10 col-md-10 col-lg-10">
          <div class="row">
          <div class="col-sm-8 col-md-2 col-lg-2 im">
            <center><img src="<%= pp.img %>" alt="no laptop"></center>
          </div>
          <div class="col-sm-10 col-md-4 col-md-4 rt">
              Rs. <%= pp.price %>
          </div>
          <br><br>
          <div class="col-sm-10 col-md-4 col-md-4 com">
            <div class="col-sm-5 col-md-2 col-lg-2"></div>
            <div class="col-sm-5 col-md-2 col-lg-2">
              <button type="button" name="button" id="atc">
                <i class="fa fa-plus-square" aria-hidden="true"></i>&nbsp;Add To Compare
              </button>
            </div>
          </div>
        </div>
        </div>
        <div class="col-sm-1 col-md-1 col-lg-1"></div>
      </div>
      <div class="row pf">
        <div class="col-sm-1 col-md-1 col-lg-1"></div>
        <div class="col-sm-10 col-md-10 col-lg-10">
          <div class="row" id="r">
            <div class="col-md-1 col-sm-1 col-lg-1"></div>
            <b>Product features:</b>
          </div>
          <div class="row" id="r2">
            <div class="col-sm-1 col-md-1 col-lg-1"></div>
            <div class="col-sm-10 col-md-5 col-lg-5">
              <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=pp.specifications.full_specs.Processor.Processor%><br>
              <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=pp.specifications.full_specs.Processor.Speed%> Clock Speed<br>
              <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=pp.specifications.full_specs.Memory.RAM%> RAM<br>
              <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=pp.specifications.full_specs.Memory['Hard Disk Capacity']%> Hard Disk<br>
            </div>
            <div class="col-sm-10 col-md-5 col-lg-5">
              <i class="fa fa-check" aria-hidden="true"></i>&nbsp;
              <% if(pp.specifications.full_specs.Graphics != undefined){ %>
              <%=pp.specifications.full_specs.Graphics.GPU%><br>
              <%}else{%>
                <%=pp.specifications.full_specs.Battery['Battery Backup']%> Battery Backup<br>
                <%}%>
              <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=pp.specifications.full_specs.Display.Size%><%=pp.specifications.full_specs.Display.Resolution%><br>
              <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=pp.specifications.full_specs.General.OS%> OS<br>
              <i class="fa fa-check" aria-hidden="true"></i>&nbsp;<%=pp.specifications.full_specs.General.Warranty%><br>
            </div>
            <div class="col-sm-1 col-md-1 col-lg-1"></div>
          </div>
          <div class="" style="height:10px"></div>
          <div class="row" id="r3">
            <div class="col-sm-1 col-lg-1 col-md-1"></div>
            <b>Full Specifications</b>
          </div>
          <br>
          <%
            var len = Object.keys(pp.specifications.full_specs).length;
            var arr = Object.keys(pp.specifications.full_specs);
            if(len%2 != 0)
            {
            for(var i=0 ; i<len-1 ; i=i+2)
            {
          %>

          <div class="row">
            <div class="col-sm-1 col-lg-1 col-md-1"></div>
            <div class="col-sm-10 col-md-5 col-lg-5">
              <div class="row heading"><b><%= arr[i]%></b></div>
              <%
              var b = "pp.specifications.full_specs.";
              var c = arr[i];
              var a = eval(b+c);
              <!-- var l = Object.keys(a).length; -->
              <!-- for(var j=0 ; j<l ; j++) -->
              <!-- var count = 0; -->
              for(var p in a)
              {
                <!-- console.log("Hetre "+count); -->
                <!-- count++; -->
                if(a.hasOwnProperty(p))
                {
              %>
              <div class="row fre">
                <div class="col-sm-12 col-md-4 col-lg-4 key">
                  <%= p%>:
                </div>
                <div class="col-sm-12 col-md-7 col-lg-7 val">
                  <%= a[p]%>
                </div>
              </div>
              <%}}%>
            </div>
            <div class="col-sm-10 col-md-5 col-lg-5">
              <div class="row heading"><b><%= arr[i+1]%></b></div>
              <%
              var d = "pp.specifications.full_specs.";
              var e = arr[i+1];
              var f = eval(d+e);
              <!-- var l1 = Object.keys(f).length; -->
              for(var p in f)
              {
                if(f.hasOwnProperty(p))
                {
              %>
              <div class="row fre">
                <div class="col-sm-12 col-md-4 col-lg-4 key">
                  <%= p%>:
                </div>
                <div class="col-sm-12 col-md-8 col-lg-8 val">
                  <%= f[p]%>
                </div>
              </div>
              <%}}%>
              </div>
            </div>

            <%}}if(len%2!=0){%>
              <div class="row">
                <div class="col-sm-1 col-lg-1 col-md-1"></div>
                <div class="col-sm-10 col-md-5 col-lg-5">
                  <div class="row heading"><b><%=arr[len-1]%></b></div>
                  <%
                  var d = "pp.specifications.full_specs.";
                  var e = arr[len-1];
                  var f = eval(d+e);
                  for(var p in f)
                  {
                    if(f.hasOwnProperty(p))
                    {
                  %>
                  <div class="row fre">
                    <div class="col-sm-12 col-md-4 col-lg-4 key">
                      <%= p%>:
                    </div>
                    <div class="col-sm-12 col-md-7 col-lg-7 val">
                      <%= f[p]%>
                    </div>
                  </div>
                  <%}}%>
                </div>
                <div class="col-sm-10 col-md-5 col-lg-5 hi">
                  <div class="row heading"><b>Processor</b></div>
                  <div class="row fre">
                    <div class="col-sm-12 col-md-4 col-lg-4 key">
                      Processor:
                    </div>
                    <div class="col-sm-12 col-md-8 col-lg-8 val">
                      AMD APU Dual Core A9 7th Gen A9-9400
                    </div>
                  </div>
                  <div class="" style="height:2px;"></div>
                  </div>
                </div>
              <%}
              if(len%2 == 0)
              {
                for(var i=0 ; i<len ; i=i+2)
                {
                  %>

                  <div class="row">
                    <div class="col-sm-1 col-lg-1 col-md-1"></div>
                    <div class="col-sm-10 col-md-5 col-lg-5">
                      <div class="row heading"><b><%= arr[i]%></b></div>
                      <%
                      var d = "pp.specifications.full_specs.";
                      var e = arr[i];
                      var f = eval(d+e);
                      for(var p in f)
                      {
                        if(f.hasOwnProperty(p))
                        {
                      %>
                      <div class="row fre">
                        <div class="col-sm-12 col-md-4 col-lg-4 key">
                          <%= p%>:
                        </div>
                        <div class="col-sm-12 col-md-7 col-lg-7 val">
                          <%= f[p] %>
                        </div>
                      </div>
                      <%}}%>
                    </div>
                    <div class="col-sm-10 col-md-5 col-lg-5">
                      <div class="row heading"><b><%= arr[i+1]%></b></div>
                      <%
                      var d = "pp.specifications.full_specs.";
                      var e = arr[i+1];
                      var f = eval(d+e);
                      for(var p in f)
                      {
                        if(f.hasOwnProperty(p))
                        {
                      %>
                      <div class="row fre">
                        <div class="col-sm-12 col-md-4 col-lg-4 key">
                          <%= p%>:
                        </div>
                        <div class="col-sm-12 col-md-8 col-lg-8 val">
                          <%= f[p]%>
                        </div>

                      </div>
                      <%}}%>
                      </div>
                    </div>

                  <%
                }
              }
              %>
            <div class="col-sm-1 col-lg-1 col-md-1"></div>
          </div>
        </div>
        <div class="col-sm-1 col-md-1 col-lg-1"></div>
      </div>
    </div>
    <div style="height:3px"></div>
    <% include footer %>
  </body>
</html>
